@Library('easyToBuy-Jenkins_SharedLib') _

pipeline {
   agent any
   stages {
      stage('Git Checkout') {
         steps {
            gitCheckout(
            branch: "master",
            url: "https://github.com/MaruthamSatishReddy/EASY-TO-BUY-V2.git",
            path: "USER-SERVICE"
            )
         }
      }
      stage('Maven Build') {
               steps {
                  mavenBuild()
               }
            }
      stage('Maven Unit Test') {
            steps {
            mavenTest()
            }
          }
      stage('Maven Integration Test') {
                  steps{
                  mavenIntegrationTest()
                  }
               }
           }
     }
